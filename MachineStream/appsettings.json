{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "MachineStreamConfiguration": {
    "ConnectionString": "ws://machinestream.herokuapp.com/ws",
    "ReconnectTimeoutSeconds": 30,
    "KeepAliveInterval": 5
  },
  "MachineDatabase": "Server=tcp:localhost,1433;Initial Catalog=machine-dev;Persist Security Info=False;User ID=admin;Password={your_password};",
  "BaseServiceSettings": {
    "UseInMemoryDatabase": true
  }
}